
	      <% if @data %>
	      <% @data.each do |d| %>
	      <tr>
		 <td style="text-align:center;"><%= d.x.round(3) %></td>
		 <td></td>
		 <td style="text-align:center;"><%= (d.y).round(1) %></td>
		 <td><%= link_to "remover", d, remote: true, method: :delete, confirm: "Pretende remover este ponto?", class: "thermo_remove_data_point_button" %></td>
	      </tr>
	      <% end %>
	      <% end %>

