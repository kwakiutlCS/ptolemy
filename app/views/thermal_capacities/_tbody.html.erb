<tr>
		 <td>
		   <%= number_field_tag(:volume, nil,  class:"thermo_input", id:"thermo_volume_field") %>
		 </td>
		 <td>
		   <%= number_field_tag(:time, nil, class:"thermo_input", step:"any", id:"thermo_time_field") %>
		 </td>
		 <td id="thermo_energy_calculator">
		   0.0
		 </td>
		 <td>
		   <%= submit_tag(:submeter, class:"btn btn-success") %>
		 </td>
	      </tr>
	      <% if @data %>
	      <% @data.each do |d| %>
	      <tr>
		 <td style="text-align:center;"><%= d.x.round(0) %></td>
		 <td></td>
		 <td style="text-align:center;"><%= (d.y/1000).round(1) %></td>
		 <td></td>
	      </tr>
	      <% end %>
	      <% end %>

