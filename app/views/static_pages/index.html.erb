


<section class="row">

<section class="span3" id="template_navbar">

  <div id="template_general_navbar">
    <% @subcategories.each do |s| %>
    <p><%= link_to s.title, static_pages_path(category: s.id), remote:true %></p>
    <% end %>
  </div>
  <div id="template_specific_navbar">
    <%= link_to "voltar", remote:true %>
  </div>
</section>

<section class="span9" id="template_index">
  <%= render "templates" %>

</section>



</section>

<canvas id="mycanvas" style="background:#ddd; width:500px; height: 300px;"></canvas>
<img class="paper_img" src="assets/statue.png" id="statue"></img>
<script type="text/paperscript" canvas="mycanvas">
  
  var statue = new Raster("statue");
  statue.position = new Point(250,150);

  var statue2 = new Raster("statue");
  
  var statue3 = new Raster("statue");
  statue3.position = view.center +new Point(200,0);
</script>
