$(function() {

    $(".background").on("click", ".background_next", function() {
	 $(this).slideUp();
	 $(".strategy").slideDown();
    });

    $(".strategy").on("click", ".strategy_previous", function() {
	 $(this).slideUp();
	 $(".background").slideDown();
    });

    $(".strategy").on("click", ".strategy_next", function() {
	 $(this).slideUp();
	 //$().slideDown();
    });


});